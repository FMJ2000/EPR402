dish#           := interpolate(1/36pt#)(.7, .8, .9, 1, 1, 1, 1);
bracket#        := interpolate(1/36pt#)(10, 12, 14, 16, 18, 20, 24);
jut#            := interpolate(1/36pt#)(17, 19, 21, 24, 27, 30, 36);
