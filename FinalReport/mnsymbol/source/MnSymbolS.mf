comma_depth#    := interpolate(1/36pt#)(35, 42, 49, 56, 63, 70, 84);
desc_depth#     := interpolate(1/36pt#)(35, 42, 49, 56, 63, 70, 84);

crisp#          := interpolate(1/36pt#)(6,   6.5, 7,    8,  8,  8,  9);
tiny#           := interpolate(1/36pt#)(6,   6.5, 7,    8,  8,  8,  9);
